{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js"
    },
    {
      "src": "/api/batch/(.*)",
      "dest": "index.js",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/dashboard.html",
      "dest": "public/dashboard.html",
      "headers": {
        "Cache-Control": "public, max-age=0"
      }
    },
    {
      "src": "/batch.html",
      "dest": "public/batch.html",
      "headers": {
        "X-Frame-Options": "DENY",
        "Cache-Control": "public, max-age=0"
      }
    },
    {
      "src": "/index-auth.html",
      "dest": "public/index-auth.html",
      "headers": {
        "Cache-Control": "public, max-age=0"
      }
    },
    {
      "src": "/admin.html",
      "dest": "public/admin.html",
      "headers": {
        "Cache-Control": "public, max-age=0"
      }
    },
    {
      "src": "/test.html",
      "dest": "public/test.html",
      "headers": {
        "Cache-Control": "public, max-age=0"
      }
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}